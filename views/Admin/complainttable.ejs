<html>
  <head>
    <%-include("../Homepage/Header") -%> 
    <link rel="stylesheet" href="/css/navandside.css">
    <link rel="stylesheet" href="/css/complainttable.css">
  </head>
  <body>
    <%-include("../Homepage/Nav") -%> 
	
    <div>
    <%-include("../Homepage/sidebar")-%>
    </div>
	<h2>Complaint Table <Table></Table></h2>
   <div class="table-wrapper">
    <table class="fl-table">
        <thead>
        <tr>
            <th>SR number </th>
            <th>Name</th>
            <th>phone number</th>
            <th>Room id</th>
            <th>Item</th>
            <th>Complaint Description</th>
        </tr>
        </thead>
        <tbody>
          <% for(let x of complaints){ %> 
            <tr>
              <td> <%= x.srno %> </td>
              <td> <%=x.name  %>  </td>
              <td><%= x.phonenumber %> </td>
              <td> <%= x.room_id %>  </td>
              <td><%= x.item%></td>
              <td><%=x.complaint_desc %></td>
          </tr>
            <% } %> 
    
        
        <tbody>
    </table>
    </div>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
  </body>
  </html>